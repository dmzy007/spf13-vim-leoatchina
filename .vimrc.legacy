
Bundle 'thinca/vim-quickrun'
if count(g:spf13_bundle_groups, 'smartcomplete') && g:completable == 0
    if has('lua')
        let g:completable = 5
        Bundle 'Shougo/neocomplete.vim'
    else
        let g:completable = 6
        Bundle 'Shougo/neocomplcache.vim'
        Bundle 'JazzCore/neocomplcache-ultisnips'
    endif
endif
